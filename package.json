{
  "name": "mmmmm",
  "version": "0.0.12-alpha",
  "private": true,
  "scripts": {
    "lib": "tsc",
    "prepare": "patch-package",
    "build-rnnodeapp": "./tools/build-rnnodeapp",
    "build-worker-ios": "./tools/build-worker-ios",
    "build-worker-android": "./tools/build-worker-android",
    "propagate-replacements": "propagate-replacement-fields --field=react-native",
    "log": "adb logcat *:S nodejs:V ReactNative:V ReactNativeJS:V 'NODEJS-MOBILE'",
    "start": "npm run lib && npm run propagate-replacements && node node_modules/react-native/local-cli/cli.js start",
    "dev": "npm run build-rnnodeapp && react-native run-android && npm run log",
    "start-appium-server": "./tools/start-appium-server",
    "stop-appium-server": "./tools/stop-appium-server",
    "pretest-e2e-android": "adb uninstall com.mmmmm && npm run start-appium-server && sleep 5",
    "test-e2e-android": "tape e2e/index.js | tap-spec",
    "posttest-e2e-android": "npm run stop-appium-server",
    "update-android-version": "react-native-version --never-amend --target android",
    "clean-up-bundler": "watchman watch-del-all && rm -rf $TMPDIR/react-*",
    "clean-nodejs-mobile": "cd android && ./gradlew clean",
    "clean": "adb uninstall com.mmmmm && npm run clean-up-bundler && npm run clean-nodejs-mobile",
    "android-release": "npm run lib && cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "move-android-apk": "mkdir -p ../dat-release-android && cp android/app/build/outputs/apk/app-release.apk ../dat-release-android/",
    "commit-after-release": "./tools/commit-after-release",
    "release": "npm run update-android-version && npm run clean-up-bundler && npm run lib && npm run propagate-replacements && npm run build-worker-android && npm run android-release && npm run test-e2e-android && npm run move-android-apk && npm run commit-after-release"
  },
  "dependencies": {
    "@cycle/isolate": "3.2",
    "@cycle/run": "4.1",
    "@staltz/react-native-os": "1.2.0",
    "@staltz/react-native-workers": "0.5.3",
    "@staltz/rn-viewpager": "1.2.5",
    "@types/react": "16.0.28",
    "@types/react-native": "0.51.4",
    "@types/react-native-vector-icons": "4.2.0",
    "assert": "^1.4.1",
    "cycle-native-navigation": "2.0.0",
    "cycle-onionify": "5.0.0",
    "depject": "^4.1.0",
    "depnest": "^1.3.0",
    "multiserver": "1.10.1",
    "multiserver-worker": "1.1.0",
    "mutant": "3.21.2",
    "muxrpc": "6.4.0",
    "nodejs-mobile-react-native": "^0.1.4",
    "patchcore": "1.23.2",
    "prop-types": "15.5.x",
    "pull-flat-list": "1.2.0",
    "pull-more": "1.0.0",
    "pull-pushable": "2.1.1",
    "pull-stream": "3.6.0",
    "react": "16.0.0",
    "react-human-time": "^1.1.0",
    "react-mutant-hoc": "^1.1.0",
    "react-native": "^0.55.2",
    "react-native-dialogs": "0.0.20",
    "react-native-navigation": "1.1.375",
    "react-native-node": "3.3.0",
    "react-native-progress": "3.4.0",
    "react-native-ssb-client": "4.0.0",
    "react-native-ssb-client-keys": "1.1.0",
    "react-native-ssb-shims": "2.0.0",
    "react-native-vector-icons": "4.5.0",
    "rn-markdown": "^1.1.1",
    "spawn-sync": "^1.0.15",
    "ssb-ref": "2.9.1",
    "thread-sleep": "^2.0.0",
    "utf-8-validate": "^3.0.2",
    "xstream": "11.x",
    "xstream-from-callback": "1.0",
    "xstream-from-mutant": "1.1"
  },
  "devDependencies": {
    "appium": "1.7.2",
    "babel-preset-react-native": "4.0.0",
    "cheerio": "^1.0.0-rc.2",
    "jase": "1.2.0",
    "noderify": "3.0.2",
    "patch-package": "^5.1.1",
    "prettier": "1.5.x",
    "propagate-replacement-fields": "1.2.0",
    "react-native-version": "^2.3.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "try-thread-sleep": "^1.0.2",
    "tslint": "^5.7.0",
    "typescript": "2.4.x",
    "wd": "^1.5.0"
  },
  "react-native": {
    "os": "@staltz/react-native-os",
    "react-native-os": "@staltz/react-native-os",
    "rn-viewpager": "@staltz/rn-viewpager",
    "ssb-client": "react-native-ssb-client"
  }
}
