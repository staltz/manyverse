{
  "name": "mmmmm",
  "version": "0.0.11-alpha",
  "private": true,
  "scripts": {
    "lib": "tsc",
    "prepare": "patch-package",
    "build-worker-ios": "node node_modules/react-native/local-cli/cli.js bundle --dev false --assets-dest ./ios --entry-file lib/worker.js --platform ios --bundle-output ./ios/worker.jsbundle",
    "build-worker-android": "node node_modules/react-native/local-cli/cli.js bundle --dev false --assets-dest ./android/app/src/main/res/ --entry-file worker.js --platform android --bundle-output ./android/app/src/main/assets/worker.bundle",
    "propagate-replacements": "propagate-replacement-fields --field=react-native",
    "start": "npm run lib && npm run propagate-replacements && node node_modules/react-native/local-cli/cli.js start",
    "start-appium-server": "$(npm bin)/appium --port 4995 > appium.log & echo $! > appium-server.pid",
    "stop-appium-server": "if ps -p $(cat appium-server.pid)> /dev/null; then kill -KILL $(cat appium-server.pid); fi && rm appium-server.pid && rm appium.log",
    "pretest-e2e-android": "adb uninstall com.mmmmm && npm run start-appium-server && sleep 5",
    "test-e2e-android": "tape e2e/index.js | tap-spec",
    "posttest-e2e-android": "npm run stop-appium-server",
    "test": "jest",
    "update-android-version": "react-native-version --never-amend --target android",
    "clean-up-bundler": "watchman watch-del-all && rm -rf $TMPDIR/react-*",
    "android-release": "npm run lib && cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "move-android-apk": "mkdir -p gitignored/release && cp android/app/build/outputs/apk/app-release.apk gitignored/release/",
    "commit-after-release": "git add -A && git commit -m \"Release $(cat package.json | $(npm bin)/jase version)\" && git tag v$(cat package.json | $(npm bin)/jase version)",
    "release": "npm run update-android-version && npm run clean-up-bundler && npm run lib && npm run propagate-replacements && npm run build-worker-android && npm run android-release && npm run test-e2e-android && npm run move-android-apk && npm run commit-after-release"
  },
  "dependencies": {
    "@cycle/isolate": "3.2",
    "@cycle/run": "4.1",
    "@staltz/react-native-os": "1.2.0",
    "@staltz/react-native-workers": "0.5.3",
    "@staltz/rn-viewpager": "1.2.5",
    "@types/react": "16.0.28",
    "@types/react-native": "0.51.4",
    "@types/react-native-vector-icons": "4.2.0",
    "assert": "^1.4.1",
    "cycle-native-navigation": "2.0.0",
    "cycle-onionify": "5.0.0",
    "depject": "^4.1.0",
    "depnest": "^1.3.0",
    "multiserver": "1.10.1",
    "multiserver-worker": "1.1.0",
    "mutant": "3.21.2",
    "muxrpc": "6.4.0",
    "patchcore": "1.15.1",
    "prop-types": "15.5.x",
    "pull-flat-list": "1.2.0",
    "pull-more": "1.0.0",
    "pull-pushable": "2.1.1",
    "pull-stream": "3.6.0",
    "react": "16.0.0",
    "react-human-time": "^1.1.0",
    "react-mutant-hoc": "^1.1.0",
    "react-native": "0.51.0",
    "react-native-dialogs": "0.0.20",
    "react-native-navigation": "1.1.375",
    "react-native-progress": "3.4.0",
    "react-native-scuttlebot": "10.4.22",
    "react-native-simple-markdown": "^1.0.60-rc.3",
    "react-native-ssb-client": "3.0.2",
    "react-native-ssb-client-keys": "1.0.0",
    "react-native-ssb-shims": "2.0.0",
    "react-native-vector-icons": "4.5.0",
    "ssb-ref": "2.9.1",
    "xstream": "11.x",
    "xstream-from-callback": "1.0",
    "xstream-from-mutant": "1.1"
  },
  "devDependencies": {
    "appium": "1.7.2",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "cheerio": "^1.0.0-rc.2",
    "jase": "1.2.0",
    "jest": "20.0.4",
    "patch-package": "^5.0.0",
    "prettier": "1.5.x",
    "propagate-replacement-fields": "1.2.0",
    "react-native-version": "^2.3.1",
    "react-test-renderer": "16.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "tslint": "^5.7.0",
    "typescript": "2.4.x",
    "wd": "^1.5.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "react-native": {
    "os": "@staltz/react-native-os",
    "react-native-os": "@staltz/react-native-os",
    "rn-viewpager": "@staltz/rn-viewpager",
    "ssb-client": "react-native-ssb-client"
  }
}
